<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bzu.personalmanage.dao.AdminDao">
  <select id="login" resultType="cn.bzu.personalmanage.entity.Admin">
  select adminName,adminPass from admin where adminName=#{param1} and adminPass=#{param2}
  </select>
</mapper>